<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app version="3.0"
  xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
 
   <servlet>
      <servlet-name>GetInboxServlet</servlet-name>
      <servlet-class>com.zimbra.servlet.GetInboxServlet</servlet-class>
   </servlet>
   <servlet>
      <servlet-name>GetMailServlet</servlet-name>
      <servlet-class>com.zimbra.servlet.GetMailServlet</servlet-class>
   </servlet>

   <servlet-mapping>
      <servlet-name>GetInboxServlet</servlet-name>
      <url-pattern>/inbox</url-pattern>
   </servlet-mapping>
</web-app>